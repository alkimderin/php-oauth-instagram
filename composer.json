{
	"name": "chillerlan/php-oauth-instagram",
	"description": "A PHP 7.2+ client for the Instagram API.",
	"homepage": "https://github.com/chillerlan/php-oauth-instagram",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "dev",
	"keywords": [
		"oauth", "oauth2", "api", "client", "instagram"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-oauth/issues",
		"source": "https://github.com/chillerlan/php-oauth-instagram"
	},
	"require": {
		"php": "^7.2",
		"chillerlan/php-oauth-core": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.1",
		"chillerlan/php-database": "^2.0",
		"chillerlan/php-cache": "^1.0",
		"chillerlan/php-curl": "^1.0",
		"chillerlan/php-log": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\OAuth\\Providers\\Instagram\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\OAuthTest\\": "vendor/chillerlan/php-oauth-core/tests",
			"chillerlan\\OAuthTest\\Providers\\Instagram\\": "tests/",
			"chillerlan\\OAuthExamples\\Instagram\\": "examples/"
		}
	}
}
